#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'learning-topic==0.0.0','console_scripts','topic_helloworld_pub'
__requires__ = 'learning-topic==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('learning-topic==0.0.0', 'console_scripts', 'topic_helloworld_pub')()
    )
